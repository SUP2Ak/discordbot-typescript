    "@types/glob": "^8.1.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.25.0",
    "eslint": "9.10.0",
    "globals": "^15.12.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "discord.js": "14.16.3",
